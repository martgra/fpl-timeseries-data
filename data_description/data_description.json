            "calculated": "cumulative_sum"
        },
        "element_type": {
            "change": "season",
            "description": "Player position",
            "notes": "Could be combined with element types object to get player position in text form",
            "data_type": "integer",
            "type": "categorical",
            "calculated": "static"
        },
        "ep_next": {
            "change": "gw",
            "description": "None",
