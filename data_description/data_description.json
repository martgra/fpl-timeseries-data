        "assists": {
            "change": "gw",
            "description": "Number of assists in total",
            "notes": {
                "personal_notes": "The total number of assists during a season, except GW0 where previous season is listed",
                "official_explanation": "Total goal assists – awarded to the player from the goalscoring team who makes the final pass before a goal is scored, including own goals.",
                "referance": "https://fantasy.premierleague.com/statistics/assists"
            },
            "data_type": "integer",
            "type": "continuous",
            "calculated": "cumulative_sum"
        },
        "bonus": {
            "change": "gw",
            "description": "Total bonus points awarded",
            "notes": {
                "personal_notes": "Given based on 'bsp' score given in each match",
                "official_explanation": "The three best performing players in each match according to the BPS will receive additional bonus points - 3 points will be awarded to the highest scoring player, 2 to the second best and 1 to the third.",
                "referance": "https://fantasy.premierleague.com/statistics/bonus"
            },
            "data_type": "integer",
            "type": "continuous",
            "calculated": "cumulative_sum"
        },
        "bps": {
            "change": "gw",
            "description": "Total bonus point score",
            "notes": {
                "personal_notes": "The score/rating/rank awarded total in a season, based on score from each gw",
                "official_explanation": "The Bonus Points System (BPS) uses a range of stats to create a BPS score for each player. The three best performing players in each match will be awarded bonus points.",
                "referance": "https://fantasy.premierleague.com/statistics/bps"
            },
            "data_type": "integer",
            "type": "continuous",
            "calculated": "cumulative_sum"
        },
        "chance_of_playing_next_round": {
            "change": "continuous",
