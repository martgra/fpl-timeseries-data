            "type": "continuous",
            "calculated": "snapshot"
        },
        "first_name": {
            "change": "static",
            "description": "Player first name",
            "notes": "None",
            "data_type": "string",
            "type": "descriptive",
            "calculated": "static"
        },
        "form": {
            "change": "gw",
            "description": "Form of the player",
            "notes": "Form is a player’s average score per match, calculated from all matches played by his club in the last 30 days. https://fantasy.premierleague.com/help",
            "data_type": "float",
            "type": "continuous",
            "calculated": "moving average"
