{
    "$schema": "./data_description_schema.json",
    "line_endings": "unix",
    "elements": {
        "assists": {
            "change": "gw",
            "description": "Number of assists in total",
            "notes": "The total number of assists during a season, except GW0 where previous season is listed",
            "data_type": "integer",
            "type": "continuous",
            "calculated": "cumulative_sum"
        },
        "bonus": {
            "change": "gw",
            "description": "Total bonus points awarded",
            "notes": "Given based on 'bsp' score given in each match",
            "data_type": "integer",
            "type": "continuous",
            "calculated": "cumulative_sum"
        },
        "bps": {
            "change": "gw",
            "description": "Total bonus point score",
            "notes": "The score/rating/rank awarded total in a season, based on score from each gw",
            "data_type": "integer",
            "type": "continuous",
            "calculated": "cumulative_sum"
        },
        "chance_of_playing_next_round": {
            "change": "continuous",
            "description": "The chance of a player playing next round",
            "notes": "Seems to be not to be filled (NaN) if player have no history of not being able to play",
            "data_type": "float",
            "type": "discrete",
            "calculated": "snapshot"
        },
        "chance_of_playing_this_round": {
            "change": "None",
            "description": "The chance in range 0.0 - 100.00 of a player playing this round",
            "notes": "Seems to be not to be filled (NaN) if player have no history of not being able to play.",
            "data_type": "integer",
            "type": "discrete",
            "calculated": "snapshot"
        },
        "clean_sheets": {
            "change": "gw",
            "description": "The total number of clean sheets",
            "notes": "How many clean sheets recorded in the course of season",
            "data_type": "integer",
            "type": "None",
            "calculated": "cumulative_sum"
        },
        "code": {
            "change": "static",
            "description": "Player ID",
            "notes": "Most probably the player idenfication number",
            "data_type": "integer",
            "type": "None",
            "calculated": "static"
        },
        "corners_and_indirect_freekicks_order": {
            "change": "gw",
            "description": "Rank of the cornertakers",
            "notes": "Should apply to mainly corners - how found is unknown, perhaps number of corners taken during season",
            "data_type": "float",
            "type": "discrete",
            "calculated": "rank"
        },
        "corners_and_indirect_freekicks_text": {
            "change": "None",
            "description": "None",
            "notes": "None",
            "data_type": "None",
            "type": "None",
            "calculated": "None"
        },
        "cost_change_event": {
            "change": "continuous",
            "description": "None",
            "notes": "Difference between cost_change_event and cost_change_start unknown",
            "data_type": "integer",
            "type": "categorical",
            "calculated": "None"
        },
        "cost_change_event_fall": {
            "change": "continuous",
            "description": "None",
            "notes": "Difference between cost_change_event_fall and cost_change_start_fall unknown",
            "data_type": "integer",
            "type": "categorical",
            "calculated": "None"
        },
        "cost_change_start": {
            "change": "continuous",
            "description": "None",
            "notes": "Difference between cost_change_event and cost_change_start unknown",
            "data_type": "integer",
            "type": "categorical",
            "calculated": "None"
        },
        "cost_change_start_fall": {
            "change": "continuous",
            "description": "None",
            "notes": "Difference between cost_change_event_fall and cost_change_start_fall unknown",
            "data_type": "integer",
            "type": "categorical",
            "calculated": "None"
        },
        "creativity": {
            "change": "gw",
            "description": "Total creativity score for season",
            "notes": "Creativity assesses player performance in terms of producing goalscoring opportunities for others. It can be used as a guide to identify the players most likely to supply assists. While this analyses frequency of passing and crossing, it also considers pitch location and quality of the final ball.",
            "data_type": "float",
            "type": "continuous",
            "calculated": "cumulative_sum"
        },
        "creativity_rank": {
            "change": "gw",
            "description": "The current creativity ranking for the player",
            "notes": "Rank based on creativity total score for all players in FPL season",
            "data_type": "integer",
            "type": "discrete",
            "calculated": "rank"
        },
        "creativity_rank_type": {
            "change": "gw",
            "description": "None",
            "notes": "Other score than creativity_rank, difference not understood",
            "data_type": "integer",
            "type": "discrete",
            "calculated": "rank"
        },
        "direct_freekicks_order": {
            "change": "gw",
            "description": "Rank of the direct freekick takers",
            "notes": "How rank is calculated is unknown, perhaps number of corners taken during season",
            "data_type": "integer",
            "type": "discrete",
            "calculated": "rank"
        },
        "direct_freekicks_text": {
            "change": "None",
            "description": "None",
